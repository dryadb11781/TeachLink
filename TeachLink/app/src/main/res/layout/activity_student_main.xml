<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.a303.teachlink.StudentMain">

    <TextView
        android:id="@+id/TV_Student"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="@string/Student"
        android:textSize="22dp"/>

    <ImageButton
        android:id="@+id/IB_S_Discuss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/TV_Student"/>


    <ImageButton
        android:id="@+id/IB_S_Question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/IB_S_Discuss"/>

    <ImageButton
        android:id="@+id/IB_S_Rollcall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/IB_S_Question" />



    <ImageButton
        android:id="@+id/IB_S_Analyze"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/IB_S_Rollcall"/>

    <ImageButton
        android:id="@+id/IB_S_History"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/IB_S_Analyze"/>


</RelativeLayout>
